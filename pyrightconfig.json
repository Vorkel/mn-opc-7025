{
  "include": [".", "src", "notebooks", "api", "streamlit_app"],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.git",
    "**/.venv",
    "**/venv",
    "**/.pytest_cache"
  ],
  "pythonVersion": "3.11",
  "pythonPlatform": "Darwin",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.11",
      "extraPaths": [".", "src", "notebooks", "api"]
    }
  ],
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "autoImportCompletions": true
}
